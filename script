spawn=/usr/local/nginx/sbin/spawn-fcgi


$spawn -a 127.0.0.1 -p 9000 -f /usr/local/nginx/cgibin/post

$spawn -a 127.0.0.1 -p 9001 -f /usr/local/nginx/cgibin/sign_up

$spawn -a 127.0.0.1 -p 9002 -f /usr/local/nginx/cgibin/sign_in

$spawn -a 127.0.0.1 -p 9003 -f /usr/local/nginx/cgibin/home

$spawn -a 127.0.0.1 -p 9004 -f /usr/local/nginx/cgibin/get_all_friend_team

$spawn -a 127.0.0.1 -p 9005 -f /usr/local/nginx/cgibin/get_friends_by_team

$spawn -a 127.0.0.1 -p 9006 -f /usr/local/nginx/cgibin/query_users

$spawn -a 127.0.0.1 -p 9007 -f /usr/local/nginx/cgibin/send_friend_request

$spawn -a 127.0.0.1 -p 9008 -f /usr/local/nginx/cgibin/get_notification

$spawn -a 127.0.0.1 -p 9009 -f /usr/local/nginx/cgibin/get_notification_num

$spawn -a 127.0.0.1 -p 9010 -f /usr/local/nginx/cgibin/add_new_team

$spawn -a 127.0.0.1 -p 9011 -f /usr/local/nginx/cgibin/confirm_friend_request

$spawn -a 127.0.0.1 -p 9012 -f /usr/local/nginx/cgibin/get_latest_chat


$spawn -a 127.0.0.1 -p 9013 -f /usr/local/nginx/cgibin/get_unread_message

$spawn -a 127.0.0.1 -p 9014 -f /usr/local/nginx/cgibin/get_my_info

$spawn -a 127.0.0.1 -p 9015 -f /usr/local/nginx/cgibin/send_message

$spawn -a 127.0.0.1 -p 9016 -f /usr/local/nginx/cgibin/logout

$spawn -a 127.0.0.1 -p 9017 -f /usr/local/nginx/cgibin/get_friends_state

exit 0;

